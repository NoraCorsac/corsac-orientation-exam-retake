@model GreenFoxNotebook.Models.Ticket

<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml" lang="en">
<head>
    <title>Error reporter</title>
    <meta charset="utf-8" />
</head>
<body>
    <table border="0">
        <form action="/" method="get">
            Name:
            <select name="name">
                <option>Name: </option>
                @{foreach (var name in Model.Reporter)
                    {
                        <option value="@name">@name</option>
                    }
                }
            </select>
            Manufacturer:
            <input type="text" name="Manufacturer" value="" /> <br />
            Serial Number:
            <input type="text" name="Serial Number" value="" /> <br />
            Description:
            <input type="text" name="Description" value="" /> <br />
            <button type="submit">Report</button>
    </table>
</body>
</html>